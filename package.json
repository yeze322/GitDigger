{
  "name": "GitDigger",
  "version": "1.0.0",
  "description": "fetching github repo data by using C/S model",
  "main": "index.js",
  "repository": {},
  "author": "zeye",
  "license": "MIT",
  "scripts": {
    "dbinit": "psql -d github -f psql/createTable.sql",
    "dbclean": "psql -d github -f psql/dropTable.sql",
    "service": "npm run service:db && npm run service:url",
    "service:restart": "pm2 restart dig_svc_db dig_svc_url",
    "service:db": "pm2 start ./payload2db/executor.js --name=\"dig_svc_db\"",
    "service:url": "pm2 start ./url2actions/executor.js --name=\"dig_svc_url\"",
    "stop:db": "pm2 stop dig_svc_db",
    "stop:url": "pm2 stop dig_svc_url",
    "stop:all": "pm2 stop dig_svc_db dig_svc_url"
  },
  "dependencies": {
    "kue": "^0.11.5",
    "pg": "^6.1.2",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  }
}
