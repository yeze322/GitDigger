{
  "name": "GitDigger",
  "version": "1.0.0",
  "description": "fetching github repo data by using C/S model",
  "main": "index.js",
  "repository": {},
  "author": "zeye",
  "license": "MIT",
  "scripts": {
    "db:init": "psql -d github -f psql/createTable.sql",
    "db:clean": "psql -d github -f psql/cleanTable.sql",
    "db:reset": "psql -d github -f psql/dropTable.sql",
    "redis:flush": "redis-cli -p 3456 -n 2 flushdb",
    "start": "npm run service:db && npm run service:url",
    "restart": "pm2 stop all && pm2 flush && npm run start && npm run redis:flush",
    "stop": "pm2 stop dig_db dig_url",
    "service:db": "pm2 start ./payload2db/executor.js --name=\"dig_db\"",
    "service:url": "pm2 start ./url2actions/executor.js --name=\"dig_url\""
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "kue": "^0.11.5",
    "pg-promise": "^5.5.6",
    "redis": "^2.6.5",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  }
}
